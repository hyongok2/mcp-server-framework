{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "StreamableServer": {
    "MaxConcurrentStreams": 100,
    "StreamTimeout": "00:10:00",
    "EnableCors": true,
    "ToolAssemblies": [
      "SampleStreamableTools.dll"
    ]
  }
}